  "bugs": {
    "url": "https://github.com/amoskeyz/naija-made-backend/issues"
  },
  "homepage": "https://github.com/amoskeyz/naija-made-backend#readme",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cloudinary": "^1.18.1",
    "consola": "^2.11.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "indicative": "^7.4.2",
    "jsonwebtoken": "^8.5.1",
